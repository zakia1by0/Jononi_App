<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CB024E77"
    tools:context=".profilepage">

    <EditText
        android:id="@+id/hght"
        android:layout_width="190dp"
        android:layout_height="42dp"
        android:layout_margin="10sp"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Height : 7ft"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"

        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/conceivedate"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/conceivedate"
        android:layout_width="190dp"
        android:layout_height="42dp"
        android:layout_margin="10sp"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Conceived Date : 1/1/1971"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"

        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spousename"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <Button
        android:id="@+id/buttoncross"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_outline_cancel_24"
        app:iconTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200sp"
        android:layout_height="210sp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="120dp"
        android:background="@drawable/ic_baseline_person_24"
        android:backgroundTint="#F3FFFFFF"
        android:backgroundTintMode="src_in"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/ic_baseline_person_24" />


    <EditText
        android:id="@+id/spousename"
        android:layout_width="190dp"
        android:layout_height="42dp"
        android:layout_margin="10sp"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Spouse Name :Elon Musk"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"

        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fullname"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/fullname"
        android:layout_width="190dp"
        android:layout_height="42dp"

        android:layout_marginTop="32dp"
        android:layout_marginEnd="10dp"
        android:background="#ACBDE8"

        android:drawablePadding="10sp"
        android:hint="Name : Zeke Tamanna"
        android:inputType="text"
        android:minHeight="48dp"

        android:padding="10sp"
        android:textColor="@color/black"

        android:textColorHint="#000000"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        tools:ignore="Autofill,ExtraText,HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/email"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="120dp"
        android:background="#ACBDE8"
        android:drawableStart="@drawable/ic_baseline_email_24"

        android:drawablePadding="8sp"
        android:hint="jatka@gmail.com"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:padding="5sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"

        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="Autofill,HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="114dp"
        android:layout_height="30dp"
        android:layout_marginTop="44dp"

        android:layout_marginEnd="-19dp"
        android:text="Fullname: "
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/fullname"
        app:layout_constraintTop_toBottomOf="@+id/email"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="145dp"
        android:layout_height="25dp"
        android:layout_marginTop="15dp"

        android:layout_marginEnd="-20dp"
        android:text="Spousename: "
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/spousename"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="210dp"
        android:layout_height="46dp"
        android:layout_marginTop="27dp"

        android:layout_marginEnd="-60dp"
        android:text="Conceived Date: "
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/conceivedate"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="112dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"

        android:layout_marginEnd="-40dp"
        android:text="Height: "
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/hght"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:ignore="MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>