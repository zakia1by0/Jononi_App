<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CB024E77"
    tools:context=".profilepage">
    <EditText
        android:id="@+id/spousename3"
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginStart="35dp"

        android:layout_marginTop="4dp"
        android:layout_marginEnd="35dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Height : 7ft"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spousename2"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/spousename2"
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginStart="35dp"

        android:layout_marginTop="4dp"
        android:layout_marginEnd="35dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Conceived Date : 1/1/1971"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spousename"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <Button
        android:id="@+id/buttoncross"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_outline_cancel_24"
        app:iconTint="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200sp"
        android:layout_height="210sp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="120dp"
        android:background="@drawable/ic_baseline_person_24"
        android:backgroundTint="#F3FFFFFF"
        android:backgroundTintMode="src_in"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/ic_baseline_person_24" />


    <EditText
        android:id="@+id/spousename"
        android:layout_width="337dp"
        android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginStart="35dp"

        android:layout_marginTop="4dp"
        android:layout_marginEnd="35dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"
        android:hint="Spouse Name :Elon Musk"
        android:minHeight="48dp"
        android:padding="10sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fullname"
        tools:ignore="Autofill,HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/fullname"
        android:layout_width="337dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="35dp"
        android:layout_marginTop="30dp"

        android:layout_marginEnd="35dp"
        android:background="#ACBDE8"
        android:drawablePadding="10sp"

        android:hint="Name : Zeke Tamanna"
        android:inputType="text"
        android:minHeight="48dp"
        android:padding="10sp"

        android:textColor="@color/black"
        android:textColorHint="#000000"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        tools:ignore="Autofill,ExtraText,HardcodedText,MissingConstraints" />

    <EditText
        android:id="@+id/email"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="120dp"
        android:background="#ACBDE8"
        android:drawableStart="@drawable/ic_baseline_email_24"

        android:drawablePadding="8sp"
        android:hint="jatka@gmail.com"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:padding="5sp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="Autofill,HardcodedText,MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>